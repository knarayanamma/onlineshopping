<%=form_for @customer do |f| %>
<table class="table table-striped">
<tr>
<td><%=f.label :name%></td>
<td><%=f.text_field :name%></td>
</tr>
<tr>
<td><%=f.label :email%></td>
<td><%=f.email_field :email%></td>
</tr>
<tr>
<td><%=f.label :password%></td>
<td><%=f.password_field :password%></td>
</tr>
<tr>
<td><%=f.label :confirmpassword%></td>
<td><%=f.password_field :confirmpassword%></td>
</tr>
<tr>
<td><%=f.label :mobilenumber%></td>
<td><%=f.text_field :mobilenumber%></td>
</tr>
<tr>
<td><%=f.label :gender%></td>
<td><%=f.radio_button :gender,"male",:checked=>true%>male</td>
<td><%=f.radio_button :gender,"female",:checked=>true%>female</td>
</tr>
<tr>
<td><%=f.submit "save"%><td/>
</tr>
<table/>
<%end%>
<%=link_to "<<back",customers_path%>