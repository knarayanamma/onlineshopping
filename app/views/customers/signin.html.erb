<%=form_tag action: :signup, method: :post do%>
<table class="table table-striped">
<tr>
<td><%= label_tag :email%></td>
<td><%= email_field_tag :email%></td>
</tr>
<tr>
<td><%=label_tag :password%></td>
<td><%= password_field_tag :password%></td>
</tr>
<tr>
<td><%= submit_tag "save"%></td>
<tr>
</tr>
</table>
<%end%>
<%=link_to "back",new_customer_path%>
<%=link_to "forgetpassword",forget_password_customers_path%>

